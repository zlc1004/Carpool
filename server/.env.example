############
# Database - You can change these to any PostgreSQL database that has logical replication enabled.
############

POSTGRES_HOST=db
POSTGRES_DB=postgres
POSTGRES_PORT=5432
# default user is postgres


############
# Supavisor -- Database pooler
############
# Port Supavisor listens on for transaction pooling connections
POOLER_PROXY_PORT_TRANSACTION=6543
# Maximum number of PostgreSQL connections Supavisor opens per pool
POOLER_DEFAULT_POOL_SIZE=20
# Maximum number of client connections Supavisor accepts per pool
POOLER_MAX_CLIENT_CONN=100
# Unique tenant identifier
POOLER_TENANT_ID=your-tenant-id
# Pool size for internal metadata storage used by Supavisor
# This is separate from client connections and used only by Supavisor itself
POOLER_DB_POOL_SIZE=5


############
# API Proxy - Configuration for the Kong Reverse proxy.
############

KONG_HTTP_PORT=8000
KONG_HTTPS_PORT=8443


############
# API Gateway - Configuration for the URL paths that route to the Kong API gateway.
############

# Default values for local development
API_EXTERNAL_URL=http://localhost:8000
PUBLIC_REST_URL=http://localhost:8000/rest/v1/
SUPABASE_PUBLIC_URL=http://localhost:8000


############
# Email Auth - Configure SMTP settings to send auth confirmation emails, password recovery, etc.
############

SMTP_ADMIN_EMAIL=admin@example.com
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=admin@example.com
SMTP_PASS=password
SMTP_SENDER_NAME=CarpSchool Admin
SMTP_ADMIN_EMAIL=admin@carpool.local


############
# Authentication - Configure Auth providers
############

SITE_URL=http://localhost:3000
ADDITIONAL_REDIRECT_URLS=
JWT_EXPIRY=3600
DISABLE_SIGNUP=false
ENABLE_EMAIL_SIGNUP=true
ENABLE_EMAIL_AUTOCONFIRM=false
ENABLE_PHONE_SIGNUP=true
ENABLE_PHONE_AUTOCONFIRM=true
ENABLE_ANONYMOUS_USERS=false

# OAuth Configuration (Optional)
# ENABLE_GOOGLE_AUTH=false
# GOOGLE_CLIENT_ID=your-google-client-id
# GOOGLE_CLIENT_SECRET=your-google-client-secret

# ENABLE_APPLE_AUTH=false  
# APPLE_CLIENT_ID=your-apple-client-id
# APPLE_CLIENT_SECRET=your-apple-client-secret


############
# Studio - Configuration for Supabase Studio
############

STUDIO_DEFAULT_ORGANIZATION=CarpSchool
STUDIO_DEFAULT_PROJECT=CarpSchool


############
# Functions - Configuration for the Functions environment
############

FUNCTIONS_VERIFY_JWT=false


############
# Logs - Enable/Disable Logging
############

LOGFLARE_API_KEY=your-logflare-api-key
LOGFLARE_SOURCE_TOKEN=your-logflare-source-token
LOGFLARE_PUBLIC_ACCESS_TOKEN=your-super-secret-and-long-logflare-key
LOGFLARE_PRIVATE_ACCESS_TOKEN=your-super-secret-and-long-logflare-key

############
# Storage
############

# Configure S3 backend for file storage (optional)
# STORAGE_BACKEND=file
# GLOBAL_S3_BUCKET=your-s3-bucket-name
# REGION=us-east-1
# AWS_ACCESS_KEY_ID=your-access-key
# AWS_SECRET_ACCESS_KEY=your-secret-key

# File size limits
FILE_SIZE_LIMIT=52428800


############
# External Services for CarpSchool
############

# Push Notifications (Optional - for production)
ONESIGNAL_APP_ID=your-onesignal-app-id
ONESIGNAL_API_KEY=your-onesignal-api-key
FIREBASE_ADMIN_CREDENTIALS=your-firebase-admin-credentials-json

# Maps & Geocoding (Optional)
GEOCODING_API_KEY=your-geocoding-api-key
MAPS_API_KEY=your-maps-api-key

# Rate Limiting for API endpoints
RATE_LIMIT_REQUESTS_PER_MINUTE=60
RATE_LIMIT_WINDOW_SIZE_MINUTES=1

# Environment
NODE_ENV=development


############
# Docker socket location (for rootless docker)
############

# If using rootless Docker, set the socket location (uncomment and modify as needed)
# DOCKER_SOCKET_LOCATION=/run/user/1000/docker.sock


############
# Advanced Configuration
############

# Database configuration
PGRST_DB_SCHEMA=public,storage,graphql_public
PGRST_DB_ANON_ROLE=anon
PGRST_DB_USE_LEGACY_GUCS=false
PGRST_APP_SETTINGS_JWT_SECRET=super-secret-jwt-token-with-at-least-32-characters-long
PGRST_APP_SETTINGS_JWT_EXP=3600

# Analytics
LOGFLARE_LOGGER_BACKEND_API_KEY=your-logflare-api-key

# Misc
ENABLE_IMAGE_TRANSFORMATION=true
